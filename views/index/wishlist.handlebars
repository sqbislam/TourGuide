<!DOCTYPE html>
<html>
<head>
	<title>WishList</title>
</head>
<body>

{{#each wish}}

	<div class="container" style="margin-top: 10px">
		<div class="card black-text">
				<div class="card-content">
					<h5> Accomodation </h5>
					<ul>
						<li>Name: {{accom.name}}</li>

						<li>Type: {{accom.type}}</li>

						<li>Price: {{accom.price}}</li>

						<li>ID: {{accom.id}}</li>
					</ul>

				</div>
		</div>


		<div class="card black-text">
				<div class="card-content">
					<h5> Transport </h5>
					<ul>
						<li>Name: {{transport.name}}</li>

						<li>Category: {{transport.category}}</li>

						<li>Price: {{transport.price}}</li>

						<li>Destination: {{transport.location}}</li>
					</ul>

				</div>
		</div>
	</div>
	<hr>
{{/each}}


</body>
</html>